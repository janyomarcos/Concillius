<!DOCTYPE html>
<html lang="br">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    
    <script src="@Url.Content("~/Scripts/jquery-1.7.1.js")" type="text/javascript"></script>    
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <link  href="@Url.Content("~/Content/themes/bootstrap/css/bootstrap.min.css")" rel="stylesheet" type="text/css"/>
    <link href="@System.Web.Optimization.BundleTable.Bundles.ResolveBundleUrl("~/Content/css")" rel="stylesheet" type="text/css" />
    <link href="@System.Web.Optimization.BundleTable.Bundles.ResolveBundleUrl("~/Content/themes/base/css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/themes/jquery/jquery-1.10.1.css")" rel="stylesheet" type="text/css"/>
    <link  href="@Url.Content("~/Content/Concillius.css")" rel="stylesheet" type="text/css"/>
    <link  href="@Url.Content("~/Content/forms.css")" rel="stylesheet" type="text/css"/>
    <meta name="viewport" content="width=device-width" />

</head>
   
    <body>
        <div class="row-fluid">
            <div class="span12">
            <div id="topo100" class="linkTopo">
                <div style="color: white; text-align: right; padding-right: 110px;">
                    <a  href="@Url.Action("PaginaInicial","Home")" style="text-decoration: none; color: white;"><img src="~/Images/pagina_inicial.gif" />&nbsp;&nbsp;Home</a>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                    Usuário: @Session["Usuario"] &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                    
                </div>
            </div>
             
                <div class="row-fluid">
                <div class="span12">
                <div class="span1"></div>
                <div id="topo" class="span10">
                    <div class="row-fluid">
                        <div class="span12">
                            <div class="span1"></div>
                            <div class="span2">
                                <h4>ATENDIMENTO AO CLIENTE</h4><br/>
                            </div>
                            <div class="span7"> </div>
                            <div class="span2">
                                <img src="~/Images/logo.ico" />
                            </div>

                        </div>
                     </div>
                </div>
                 </div>
                    <div class="row-fluid">
                        <div class="span1"></div>
                        <div class="span10">
                            @Html.Partial("_Menu")   
                            
                            <div class="article">
                                
                                <div class="row-fluid">
                                    <div class="span12 siteMap">
                                        @RenderSection("featured", required: false)
                                     </div>
                                </div>
                            
                                    <div class="row-fluid">
                                    <div class="span12">
                                        <div class="content-wrapper">
                                            @RenderBody()
                                            <div id="modal"></div>
                                            <div id="inner-modal"></div>
                                            <div id="dialog"></div>
                                            <div id="confirm"></div>
                                            <div id="domMessage" style="display:none;"><h1><img  src="~/Images/ui-anim_basic_16x16.gif" /> </h1> </div>     
                                        </div>  
                                    </div>
                                </div>
                            </div>
                            <div class="footer">
                                <p class="site-title">&copy; @DateTime.Now.Year - Concillius | contato@concillius.com.br | Tel: (85) 9988-8331</p>
                            </div>
                        </div>
                        <script src="@Url.Content("~/Scripts/jquery.meiomask.js")" type="text/javascript"></script>
                        <script src="@Url.Content("~/Scripts/jquery-ui-1.8.11.min.js")" type="text/javascript"></script>
                        <script src="@Url.Content("~/Scripts/bootstrap/bootstrap.min.js")" type="text/javascript"></script>
                        <script src="@Url.Content("~/Scripts/jquery.unobtrusive-ajax.min.js")" type="text/javascript")"></script>
                        <script src="@Url.Content("~/Scripts/cosmos-buttons.js")" type="text/javascript"></script>
                        <script src="@Url.Content("~/Scripts/onload-app.js")" type="text/javascript"></script>
                        <script src="@Url.Content("~/Scripts/MicrosoftAjax.js")" type="text/javascript"></script>
                        <script src="@Url.Content("~/Scripts/MicrosoftMvcAjax.js")" type="text/javascript"></script>
                        <script src="@Url.Content("~/Scripts/MicrosoftMvcValidation.js")" type="text/javascript"></script>
                        <script src="@Url.Content("~/Scripts/cosmos-functions.js")" type="text/javascript"></script>
                        <script src="@Url.Content("~/Scripts/cosmos-modal.js")" type="text/javascript"></script>
                        <script src="@Url.Content("~/Scripts/cosmos-accordion.js")" type="text/javascript"></script>        
                        <script src="@Url.Content("~/Scripts/cosmos-tabs.js")" type="text/javascript"></script>
                        <script src="@Url.Content("~/Scripts/listBoxDouble.js")" type="text/javascript"></script>       
                        <!--<script src="@Url.Content("~/Scripts/jquery.blockUI.min.js")"></script>
                        <script src="@Url.Content("~/Scripts/jquery.blockUI.js")"></script>-->
                    </div>
            </div>   
            </div> 
            </div>
    </body>

</html>
